<?xml version="1.0" encoding="utf-8"?>
<resources>

    <color name="blue">#0000ff</color>
    <color name="buttons_small_text_color">@color/orange</color>
    <color name="green">#00ff00</color>
    <color name="greenEnd">#ff1da130</color>
    <color name="greenMid">#ff0A8815</color>
    <color name="greenStart">#ff33dd44</color>
    <color name="green_dark">#006400</color>
    <color name="grey_dark">#2F4F4F</color>
    <color name="grey_light">#DCDCDC</color>
    <color name="grey_medium">#b7b7b7</color>
    <color name="info_header_text_color">@color/orange</color>
    <color name="orange">#ff6501</color>
    <color name="purple">#ae3ad9</color>
    <color name="red">#ff0000</color>
    <color name="yellow">#ffff00</color>

    <dimen name="activity_horizontal_margin">16dp</dimen>
    <dimen name="activity_vertical_margin">16dp</dimen>

    <string name="action_settings">Settings</string>
    <string name="app_name">Orange Lab</string>
    <string name="button_send">Send</string>
    <string name="edit_message">Enter a message</string>
    <string name="hello_world">Hello world!</string>
    <string name="s">map</string>
    <string name="title_activity_display_message">My Message</string>
    <string name="title_activity_main">Test Application</string>

    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="BarSeparator">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/grey_dark</item>
    </style>

    <style name="ButtonText">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_margin">3dp</item>
        <item name="android:textSize">30dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:shadowColor">#000000</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">2</item>
    </style>

    <style name="LightBarSeparator">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/grey_light</item>
    </style>

</resources>
